{
  "name": "feh_builder",
  "version": "1.0.0",
  "engines": {
    "node": "8.2.1",
    "npm": "5.3.0"
  },
  "description": "Team Builder for FEH",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "connect-redis": "^3.3.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "fire-emblem-heroes-stats": "^0.10.1",
    "helmet": "^1.3.0",
    "mongoose": "^4.13.5",
    "serve-favicon": "^2.4.5",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.16.0",
    "nodemon": "^1.12.1"
  },
  "scripts": {
    "buildLoginBundle": "babel ./client/login ./client/helper --presets react --out-file ./hosted/loginBundle.js",
    "buildAppBundle": "babel ./client/app ./client/helper --presets react --out-file ./hosted/bundle.js",
    "build": "npm run buildLoginBundle && npm run buildAppBundle",
    "watchLoginBundle": "babel --watch ./client/login ./client/helper --presets react --out-file ./hosted/loginBundle.js",
    "watchAppBundle": "babel --watch ./client/app .client/helper --presets react --out-file ./hosted/bundle.js",
    "nodemon": "nodemon --watch ./server ./server/app.js",
    "start": "node ./server/app.js",
    "pretest": "eslint ./server --fix",
    "test": "echo \"Tests complete\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsun6145/feh_builder.git"
  },
  "keywords": [
    "Node",
    "MVC",
    "Redis",
    "Login"
  ],
  "author": "Nathaniel Sun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nsun6145/feh_builder/issues"
  },
  "homepage": "https://github.com/nsun6145/feh_builder#readme"
}
